<div class="modal-transcript">
    <div class="modal-body">
        <h2><%- gettext('Upload New Transcript') %></h2>
        <label><%- gettext('Transcript Languages') %></label>
        <select class="js-select-language">
            <option></option>
            <% _.each(languages, function(language) { %>
            <option value="<%- language[0] %>"><%- language[1] %></option>
            <% }) %>
        </select>
    </div>
    <div class="modal-actions">
        <button class="close-button js-close-popup"><%- gettext('Close') %></button>
        <button class="next-button js-upload-transcript" disabled><%- gettext('Next') %></button>
    </div>
</div>
